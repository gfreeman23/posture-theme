    var options = {
      url: videoUrl,
      loop: videoloop,
      autoplay: autoPlay,
      autopause: autoPause,
      byline: byline,
      title: showtitle,
      muted: videoMute,
      portrait: showportrait
    };
    var player = new Vimeo.Player("vimeo-wrapper-1690600226870", options);

